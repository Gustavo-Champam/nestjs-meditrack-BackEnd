  "compilerOptions": {
    "strict": true,
    "strictPropertyInitialization": false,   // <- ADICIONE ESTA LINHA
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
